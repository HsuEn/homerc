set meta-flag on
set input-meta on
set convert-meta off
set output-meta on
set filec
set autolist                
set autologout=0

"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[5~": beginning-of-history
"\e[6~": end-of-history
"\e[3~": delete-char
"\e[2~": quoted-insert
"\e[": backward-word

"^w": backward-delete-word
"\x1b\x5b\x41": history-search-backward
"\x1b\x5b\x42": history-search-forward
